{
  "request": {
    "urlPath": "/spaces/testSpace/environments/master/entries",
    "method": "GET",
    "headers": {
      "Authorization": {
        "equalTo": "Bearer testAccessToken"
      }
    },
    "queryParameters": {
      "include": {
        "equalTo": "10"
      },
      "content_type": {
        "equalTo": "scenario"
      },
      "select": {
        "equalTo": "sys,fields.title,fields.scenarioUrl,fields.numbersFrequency,fields.rowConfig,fields.gameHarborConfig,fields.gameResourcesConfig,fields.isCitiesAndKnights"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "contentful_getContentWithFieldsQueryResponse.json"
  }
}